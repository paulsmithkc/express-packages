{
  "name": "joi-mongodb-oid",
  "version": "2.0.4",
  "private": false,
  "description": "Custom Joi validator for MongoDB ObjectId.",
  "keywords": [
    "joi",
    "mongodb",
    "objectid",
    "oid",
    "custom",
    "validator",
    "extension",
    "plugin",
    "inject"
  ],
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "npx rimraf dist",
    "build:ts": "npx tsc",
    "build:esm": "npx tsup --format esm --minify --env.NODE_ENV production",
    "build:cjs": "npx tsup --format cjs --minify --env.NODE_ENV production",
    "test": "npx jest --collectCoverage",
    "example:cjs": "npm run build && node ./examples/example.cjs",
    "example:mjs": "npm run build && node ./examples/example.mjs",
    "pretest": "npm run build",
    "prepack": "npm run build",
    "prepublish": "npm run build",
    "preexample": "npm run build"
  },
  "author": "Paul Smith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulsmithkc/express-packages"
  },
  "bugs": {
    "url": "https://github.com/paulsmithkc/express-packages/issues"
  },
  "homepage": "https://github.com/paulsmithkc/express-packages/tree/main/packages/joi-mongodb-oid#readme",
  "peerDependencies": {
    "joi": ">=17.0.0",
    "mongodb": ">=4.0.0"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest",
    "typescript": "latest"
  }
}
